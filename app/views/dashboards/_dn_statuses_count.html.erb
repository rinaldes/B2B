<table class="table table-bordered table-striped status">
    <thead>
      <tr>
        <th class="hidden-phone" width="20%">
          <i class="icon-caret-right blue"></i>
        </th>
        <th>
          <i class="icon-caret-right blue"></i>
          Description
        </th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td class="hidden-phone">
          <span class="label label-large label-yellow arrowed arrowed-in">pending</span>
        </td>
        <td><%=count[:pending]%> Pending</td>
      </tr>
      <tr>
        <td class="hidden-phone">
          <span class="label label-large label-warning arrowed arrowed-in ">disputed</span>
        </td>
        <td><%=count[:disputed]%> Disputed</td>
      </tr>
      <tr>
        <td class="hidden-phone">
          <span class="label label-large label-pink arrowed arrowed-in">revisioned</span>
        </td>
        <td><%=count[:revisioned]%> Revisioned </td>
      </tr>
