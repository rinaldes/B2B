<div class="modal-800">
   <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
    <h3 id="myModalLabel">List Detail Bank</h3>
   </div>
  <div class="modal-body">
    <div class="row-fluid">
      <div class="span12">
        <fieldset>
          <div class="control-group">
            <label> Receipt Bank:</label>
            <div class="control">
              <strong><%= bank.receive_bank_name %></strong>
            </div>
            <span class="help-block red"><% %></span>
          </div>
          <div class="control-group">
            <label> Receipt Account Number:</label>
            <div class="control">
              <strong><%= bank.receive_bank_rek %></strong>
            </div>
            <span class="help-block red"><% %></span>
          </div>
          <div class="control-group">
            <label> From Bank:</label>
            <div class="control">
              <strong><%= bank.from_bank_name %></strong>
            </div>
            <span class="help-block red"><% %></span>
          </div>
          <div class="control-group">
            <label> Account Bank :</label>
            <div class="control">
              <strong><%= bank.from_bank_rek %></strong>
            </div>
            <span class="help-block red"><% %></span>
          </div>
          <div class="control-group pull-right">
            <div class="control">
              <span id="error-message"></span>
              <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
            </div>
          </div>
        </fieldset>
      </div>
    </div>
  </div>
</div>

