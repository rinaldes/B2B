<div id="revise-dispute">
  <div class="hr hr8 hr-double hr-dotted"></div>
  <div class="row-fluid">
    <div class="span12">
      <h4 class="lighter">Remarked</h4>
      <p>Delivered quantities are confirmed and signed off for</p>
    </div>
  </div>
  <div class="row-fluid">
    <div class="span7 pull-left">
      <% if can?(:get_revision_debit_note, @debit_note) && !dn_escalated %>
        <% if @f_details.count != 0 %>
          <%=link_to "Reject", order_to_payments_new_revise_dispute_dn_path(@debit_note.id), class: "btn btn-success", remote:true %>
        <% end %>
      <%- end%>
    </div>
  </div>
</div>