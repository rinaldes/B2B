<div class="row-fluid">
	<div class="span12">
	  <div id="table-dn" class="dataTables_wrapper"role="grid">
		<table class="table table-striped table-bordered table-hover" id="table-dn-results">
		  <thead>
		    <tr>
		      <th class="center">Debit Note No</th>
		      <th class="center">Supplier Code</th>
		      <th class="center">Total</th>
		      <th class="center">
		        <%= check_box_tag "select_all_dn", "", false,:onclick=>"to_select_toggle(this)", :class=>"add_dn" %>
		        <span class="lbl"></span>
		      </th>
		    </tr>
		  </thead>
		  <tbody id="check_dn">
        <tr>
        	<td colspan="4"><center><span class="red">No match data</span></center></td>
        </tr>
		  </tbody>
		</table>
		<div id="btn-add-dn"></div>
		<div id="link-to-load-dn"></div>
	  </div>
	</div>
</div>