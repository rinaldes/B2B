<div class="row-fluid">
	<div class="span12">
	  <div id="table-retur" class="dataTables_wrapper"role="grid">
		<table class="table table-striped table-bordered table-hover" id="table-retur-results">
		  <thead>
		    <tr>
		      <th class="center">Retur No</th>
		      <th class="center">Supplier Code</th>
		      <th class="center">Warehouse Code</th>
		      <th class="center">Total</th>
		      <th class="center">
		        <%= check_box_tag "select_all_retur", "", false, :onclick => "to_select_toggle(this)", :class=>"add_retur" %>
		        <span class="lbl"></span>
		      </th>
		    </tr>
		  </thead>
		  <tbody id="check_retur">
        <tr>
        	<td colspan="5"><center><span class="red">No match data</span></center></td>
        </tr>
		  </tbody>
		</table>
		<div id="btn-add-retur"></div>
		<div id="link-to-load-retur"></div>
	  </div>
	</div>
</div>