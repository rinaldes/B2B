$("#accept-or-dispute").hide("slow");
$("#retur-details").html("<%=escape_javascript(render 'form_dispute_return', :retur_dispute => @retur_dispute, :details => @details, :retur => @retur)%>");
<%if @retur_dispute.errors.any?%>
  $("#accept-dispute").append("<span class='red'>There is one or more errors, please try again</span>");
<%end%>
$('#details-retur-table').dataTable({
    "bPaginate": true,
    "bLengthChange": true,
    "bFilter": true,
    "bInfo": false,
    "iDisplayLength": 10});
