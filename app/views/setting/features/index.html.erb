<div class="row-fluid">
  <div class="space-6"></div>
  <div class="row-fluid">
    <div class="span12">
      <%=render 'shared/filter_and_search_table'%>
      <div id="table_report_wrapper" class="dataTables_wrapper" role="grid">
        <table class="table table-striped table-bordered table-hover" id="results">
          <thead>
            <tr>
              <th class="center" >
                <%= link_to raw('Feature Name'), 'javascript:void(0)', id: 'name-desc', class: 'sorting hide' %>
                <%= link_to raw('Feature Name'), 'javascript:void(0)', id: 'name-asc', class: 'sorting' %>
              </th>
              <th class="center">
                <%= link_to raw('Regulator'), 'javascript:void(0)', id: 'regulator-desc', class: 'sorting hide' %>
                <%= link_to raw('Regulator'), 'javascript:void(0)', id: 'regulator-asc', class: 'sorting' %>
              </th>
              <th class="center">
                <%= link_to raw('Description'), 'javascript:void(0)', id: 'description-desc', class: 'sorting hide' %>
                <%= link_to raw('Description'), 'javascript:void(0)', id: 'description-asc', class: 'sorting' %>
              </th>
              <th class="center">
                <%= link_to raw('Last Update'), 'javascript:void(0)', id: 'updated_at-desc', class: 'sorting hide' %>
                <%= link_to raw('Last Update'), 'javascript:void(0)', id: 'updated_at-asc', class: 'sorting' %>
              </th>
              <th class="center">Action</th>
            </tr>
            <tr>
              <th><%= text_field_tag "name", params[:name], id:"name",:class=>'auto-complete search', :autocomplete=>:off, :onkeypress => 'search_data(event)', :style => "width: 90%" %></th>
              <th><%= text_field_tag "regulator", params[:regulator], id:"regulator",:class=>'auto-complete search', :autocomplete=>:off, :onkeypress => 'search_data(event)', :style => "width: 90%" %></th>
              <th></th>
              <th><%= text_field_tag "updated_at", params[:updated_at], id:"updated_at",:class=>'auto-complete search', :autocomplete=>:off, :onkeypress => 'search_data(event)', :style => "width: 90%" %></th>
              <th></th>
            </tr>
          </thead>
          <tbody id="feature-list"><%=render "results", :results => @results %></tbody>
        </table>
        <%=render "shared/paginate", :results => @results %>
      </div>
    </div>
  </div>
    <!--PAGE CONTENT ENDS HERE-->
</div><!--/row-->

<script>
  function delete_feature(path, id){
    _remove_data(path, id)
  }
</script>
