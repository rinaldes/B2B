<div class="row-fluid">
  <div class="space-6"></div>
  <div class="row-fluid">
    <div class="span12">
      <%= render 'shared/filter_and_search_table'%>
      <div id="table_report_wrapper" class="dataTables_wrapper" role="grid">
        <table class="table table-striped table-bordered table-hover" id="results">
          <thead>
            <tr>
              <th class="center">
                <%= link_to raw('Title'), 'javascript:void(0)', id: 'title-desc', class: 'sorting hide' %>
                <%= link_to raw('Title'), 'javascript:void(0)', id: 'title-asc', class: 'sorting' %>
              </th>
              <th class="center">
                <%= link_to raw('Description'), 'javascript:void(0)', id: 'description-desc', class: 'sorting hide' %>
                <%= link_to raw('Description'), 'javascript:void(0)', id: 'description-asc', class: 'sorting' %>
              </th>
              <th class="center">
                <%= link_to raw('Valid Until'), 'javascript:void(0)', id: 'valid_until-desc', class: 'sorting hide' %>
                <%= link_to raw('Valid Until'), 'javascript:void(0)', id: 'valid_until-asc', class: 'sorting' %>
              </th>
              <th class="center">Action</th>
            </tr>
            <tr>
              <th><%= text_field_tag "title", params[:title], id:"title",:class=>'auto-complete search', :autocomplete=>:off, :onkeypress => 'search_data(event)', :style => "width: 200px" %></th>
              <th><%= text_field_tag "description", params[:description], id:"description",:class=>'auto-complete search', :autocomplete=>:off, :onkeypress => 'search_data(event)', :style => "width: 200px" %></th>
              <th><%= text_field_tag "valid_until", params[:valid_until], id:"valid_until",:class=>'auto-complete search', :autocomplete=>:off, :onkeypress => 'search_data(event)', :style => "width: 200px" %></th>
              <th></th>
            </tr>
          </thead>
          <tbody id="image-blast-list"><%=render "results", :results => @results %></tbody>
        </table>
        <%= render "shared/paginate", :results => @results %>
      </div>
    </div>
  </div>
      <!--PAGE CONTENT ENDS HERE-->
</div><!--/row-->
<script>
  function delete_image(path, id){
    _remove_data(path, id)
  }
</script>