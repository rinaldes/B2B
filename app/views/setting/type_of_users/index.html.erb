<div class="row-fluid">
  <div class="space-6"></div>
  <div class="row-fluid">
    <div class="span12">
      <%= render 'shared/filter_and_search_table'%>
      <div id="table_report_wrapper" class="dataTables_wrapper" role="grid">
        <table class="table table-striped table-bordered table-hover" id="results">
          <thead>
            <tr>
              <th class="center">
                <%= link_to raw('Group Type'), 'javascript:void(0)', id: 'parent_id-desc', class: 'sorting hide' %>
                <%= link_to raw('Group Type'), 'javascript:void(0)', id: 'parent_id-asc', class: 'sorting' %>
              </th>
              <th class="center">
                <%= link_to raw('Group'), 'javascript:void(0)', id: 'description-desc', class: 'sorting hide' %>
                <%= link_to raw('Group'), 'javascript:void(0)', id: 'description-asc', class: 'sorting' %>
              </th>
              <th class="center">Action</th>
            </tr>
            <tr>
              <th><%= text_field_tag "parent_id", params[:parent_id], id:"parent_id",:class=>'auto-complete search', :autocomplete=>:off, :onkeypress => 'search_data(event)', :style => "width: 450px" %></th>
              <th><%= text_field_tag "description", params[:description], id:"description",:class=>'auto-complete search', :autocomplete=>:off, :onkeypress => 'search_data(event)', :style => "width: 450px" %></th>
              <th></th>
            </tr>
          </thead>
          <tbody id="type-of-user-list"><%=render "results", :results => @results %></tbody>
        </table>
        <%= render "shared/paginate", :results => @results %>
      </div>
    </div>
  </div>
      <!--PAGE CONTENT ENDS HERE-->
</div><!--/row-->
