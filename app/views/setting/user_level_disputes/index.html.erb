<div class="row-fluid">
  <div class="space-6"></div>
  <div class="row-fluid">
    <div class="span12">
      <%= render 'shared/filter_and_search_table'%>
      <div id="table_report_wrapper" class="dataTables_wrapper" role="grid">
        <table class="table table-striped table-bordered table-hover" id="results">
          <thead>
            <tr>
              <th class="center">
                <%= link_to raw('Group'), 'javascript:void(0)', id: 'user_id-desc', class: 'sorting hide' %>
                <%= link_to raw('Group'), 'javascript:void(0)', id: 'user_id-asc', class: 'sorting' %>
              </th>
              <th class="center">
                <%= link_to raw('User'), 'javascript:void(0)', id: 'user_id-desc', class: 'sorting hide' %>
                <%= link_to raw('User'), 'javascript:void(0)', id: 'user_id-asc', class: 'sorting' %>
              </th>
              <th class="center">
                <%= link_to raw('Level'), 'javascript:void(0)', id: 'level-desc', class: 'sorting hide' %>
                <%= link_to raw('Level'), 'javascript:void(0)', id: 'level-asc', class: 'sorting' %>
              </th>
              <th class="center">
                <%= link_to raw('Dispute Setting'), 'javascript:void(0)', id: 'dispute_setting_id-desc', class: 'sorting hide' %>
                <%= link_to raw('Dispute Setting'), 'javascript:void(0)', id: 'dispute_setting_id-asc', class: 'sorting' %>
              </th>
              <th class="center">Action</th>
            </tr>
          </thead>
          <tbody id="user-level-dispute-list"><%=render "results", :results => @results %></tbody>
        </table>
        <%= render "shared/paginate", :results => @results %>
      </div>
    </div>
  </div>
      <!--PAGE CONTENT ENDS HERE-->
</div><!--/row-->
<script>
  function delete_uld(path, id){
    _remove_data(path, id)
  }
</script>
