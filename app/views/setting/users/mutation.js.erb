<%if @flag == "mutation_to_another_supplier"%>
  $('#modal-mutation').html('<%= escape_javascript(render("suppliers", :flag => @flag, :results => @results)) %>');
  $("#input-for-supplier-or-warehouse").html("<%= escape_javascript(hidden_field_tag 'supplier[supplier_id]') %>");
<% elsif @flag == "mutation_to_another_warehouse"%>
  $('#modal-mutation').html('<%= escape_javascript(render("warehouses", :flag => @flag, :results => @results)) %>');
  $("#input-for-supplier-or-warehouse").html("<%= escape_javascript(hidden_field_tag 'warehouse[warehouse_id]') %>")
<%end%>