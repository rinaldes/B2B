<%- if @supplier.errors.any? %>
	<% if @supplier.errors.messages[:priority].present? %>
		$('#error_barcode').html("<%= @supplier.errors.messages[:priority].join(', ') %>");
	<% else %>
		$('#modal-edit-barcode-setting').html("<%= escape_javascript(render('edit')) %>");
	<% end %>
<%else%>
  $('#modal-edit-barcode-setting').modal('hide');
  var row = '<%=@supplier.id%>';
  $('#table_report_wrapper #row_'+row+' .level').html('<%=@supplier.level%>');
  $('#table_report_wrapper #row_'+row+' .last-update').html('<%= time_ago_in_words(@supplier.updated_at)%> ago ');
  $("#flash-message").html(create_alert_notice_for_js("Supplier's level was successfully changed"));
<%-end%>
