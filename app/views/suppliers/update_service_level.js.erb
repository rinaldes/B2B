<%- if @supplier.errors.any? %>
  $("#error-message").addClass("alert alert-error").html("<%=@supplier.errors[:service_level_code].join(' ')%>");
<%else%>
  $('#modal-edit-service-level').modal('hide');
  var row = '<%=@supplier.id%>';
  $('#table_report_wrapper #row_'+row+' .level').html('<%=@supplier.level%>');
  $('#table_report_wrapper #row_'+row+' .last-update').html('<%= time_ago_in_words(@supplier.updated_at)%> ago ');
  $("#flash-message").html(create_alert_notice_for_js("Supplier's level was successfully changed"));
  window.location.hash = "#flash-message";
<%-end%>
