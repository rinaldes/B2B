<%- if @group.errors.any? %>
  $('#modal-edit-level').html("<%= escape_javascript(render('edit')) %>");
<%else%>
  $('#modal-edit-level').modal('hide');
  //$('#supplier-group-list').html('<%= escape_javascript(render("results", :results => @results)) %>');

  var row = '<%=@group.id%>';
  $('#table_report_wrapper #row_'+row+' .level').html('<%=@group.level%>');
  $('#table_report_wrapper #row_'+row+' .last-update').html('<%= time_ago_in_words(@group.updated_at)%> ago ');
  $("#flash-message").html(create_alert_notice_for_js("Supplier's level was successfully changed"));
<%-end%>
