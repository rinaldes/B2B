<div class="modal-800">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
    <h3 id="myModalLabel">Setting Area</h3>
  </div>
  <div id="message_error"></div>
  <div class="modal-body">
    <div id="table_report_wrapper_area" class="dataTables_wrapper" role="grid">
      <table class="table table-striped table-bordered table-hover">
      <thead>
        <tr>
          <th class="center">Area Code</th>
          <th class="center">Area Name</th>
          <th class="center">Select</th>
        </tr>
      </thead>
      <tbody id="container_area">
        <%= render "get_area", :results => results %>
      </tbody>
    </table>
    <%= hidden_field_tag "tmp_selected_area" %>
    <%= hidden_field_tag "tmp_warehouse_id", params[:warehouse_id] %>
    <%=render "paginate_areas", :results => results %>
    <div class="row-fluid as">
      <%= button_tag "Cancel", :class => "btn btn-small pull-right", "data-dismiss" => "modal" %>
      <%= button_tag "Update", :id => "submit_btn", :class => "btn btn-info btn-small pull-right", :onclick => "updateWh()" %>
    </div>
  </div>
</div>