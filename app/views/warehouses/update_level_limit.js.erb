$("#update_limit_date").html("<%= escape_javascript(render 'form_level_limit', :level_limits => @level_limits) %>");
var _status_return = "<%= @update_limit_date.join(',') %>";
var counter = 0;
var c = 0;
var check_status_true = checkArrayFind(_status_return);
if (check_status_true == -1){
  $("#flash-message").html(create_alert_notice_for_js("Successfully, Thanks for updated it"));
  window.location.hash = '#flash-message';
}else{
  _status_return.split(",").map(function(index){
    if (index == "false"){
      $($(".alert_notice")[counter]).html("Failed, please check again.")
    }
    counter += 1;
  });
}

function checkArrayFind(status_return){
  return jQuery.inArray( "false", status_return.split(",") )
}